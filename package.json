{
  "name": "cooltheme",
  "version": "1.0.0",
  "description": "A wordpress theme",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all compile:sass prefix:css compress:css",
    "compile:sass": "node-sass sass/main.scss css/style.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" -o css/style.prefix.css css/style.css",
    "compress:css": " node-sass css/style.prefix.css css/style.min.css --output-style compressed",
    "babel": "babel js/main.js -o js/main.min.js --watch",
    "sass": "node-sass sass/main.scss style.css -w",
    "dev": "npm-run-all --parallel sass babel "
  },
  "author": "digital villa",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.4.10",
    "concat": "^1.0.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  },
  "browserslist": {
    "production": [
      "last 10 version"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1"
  }
}
